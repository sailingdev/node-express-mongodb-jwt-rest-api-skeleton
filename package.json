    "date-fns": "^1.29.0",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "helmet": "^3.14.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.6",
    "mongoose-paginate-v2": "^1.0.12",
    "morgan": "^1.9.1",
    "nodemailer": "^4.6.8",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
