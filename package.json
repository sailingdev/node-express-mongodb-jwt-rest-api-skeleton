{
  "name": "node-express-mongodb-jwt-rest-api-skeleton",
  "version": "0.3.0",
  "description": "Node.js express.js MongoDB JWT REST API - Basic Project Skeleton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davellanedam/node-express-mongodb-jwt-rest-api-skeleton.git"
  },
  "scripts": {
    "start": "NODE_ENV=production pm2 start server.js --watch",
    "test": "mocha --no-deprecation --timeout=5000",
    "dev": "NODE_ENV=development nodemon --no-deprecation server.js",
    "fresh": "npm run clean && npm run seed",
    "clean": "node --no-deprecation clean.js",
    "seed": "node --no-deprecation seed.js",
    "format": "prettier --write --config .prettierrc.json \"**/*.js\""
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv-safe": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.14",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "nodemailer": "^4.6.8",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "request-ip": "^2.1.1",
    "trim-request": "^1.0.6",
    "uuid": "^3.3.2",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "eslint": "^5.5.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "mongo-seeding": "^2.2.0",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2"
  }
}
